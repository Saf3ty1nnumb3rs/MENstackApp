<main>

	<h2>Add A New Whisky to the WISHkey List!</h2>

	<a href="/">Return to Home Page</a>

	<div id="edit-form" class="user-container">

		<form method="POST" action="./">
			<div class="form-box">
				<div>
					<label for="name">Name of Whisky</label>
					<input type="text" name="name">
				</div>
				<div>
					<label for="img">Image URL</label>
					<input type="text" name="img">
				</div>
				<div>
					<label for="yearsAged">Years Aged</label>
					<input type="text" name="yearsAged">
				</div>
				<div>
					<label for="originCountry">Country</label>
					<input type="text" name="originCountry">
				</div>
				<div>
					<label for="style">Style</label>
					<input type="text" name="style">
				</div>
				<div>
					<label for="distillery">Distillery</label>
					<input type="text" name="distillery">
				</div>
				<div>
					<label for="yearMade">Distillery Found</label>
					<input type="text" name="yearMade">
				</div>

				<button type="submit">Add Whisky</button>
			</div>
		</form>

	</div>
</main>